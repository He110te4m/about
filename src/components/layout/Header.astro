---
import HeaderLink from "@/components/HeaderLink.astro";
import { SITE_TITLE } from "@/consts";
import NavList from '@/components/nav/NavList.astro';

const navs = [
  { url: '/', title: 'Home' },
  { url: '/posts', title: 'Posts' },
  { url: '/about', title: 'About' },
]

---

<header>
  <h2>
    {SITE_TITLE}
  </h2>
  <NavList navs={navs} />
</header>
<style>
  header {
    margin: 0em 0 2em;
  }
  h2 {
    margin: 0.5em 0;
  }
</style>
